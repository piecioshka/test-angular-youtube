{
    "private": true,
    "name": "test-angular-youtube",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:staging": "http-server dist/test-angular-youtube/ -d false -g",
        "build": "ng build --base-href /test-angular-youtube/",
        "build:staging": "ng build --configuration=staging",
        "build:production": "npm run build -- --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "clear": "rm -rf dist/",
        "clear:all": "rm -rf node_modules/ && npm run clear",
        "predeploy": "npm run build:production",
        "deploy": "cross-env NODE_DEBUG=gh-pages gh-pages -d dist/test-angular-youtube/",
        "heroku-postbuild": "npm run build:staging",
        "deploy:staging": "git push heroku master -f",
        "build:mock": "node data/build-database.js",
        "start:mock": "node data/start-database.js"
    },
    "dependencies": {
        "@angular/common": "^7.1.4",
        "@angular/compiler": "^7.1.4",
        "@angular/core": "^7.1.4",
        "@angular/forms": "^7.1.4",
        "@angular/http": "^7.1.4",
        "@angular/platform-browser": "^7.1.4",
        "@angular/platform-browser-dynamic": "^7.1.4",
        "@angular/router": "^7.1.4",
        "core-js": "^2.6.1",
        "http-server": "^0.11.1",
        "ngx-pagination": "^3.2.1",
        "rxjs": "^6.3.3",
        "tslib": "^1.9.3",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.11.4",
        "@angular/cli": "^7.1.4",
        "@angular/compiler-cli": "^7.1.4",
        "@angular/language-service": "~7.1.0",
        "@types/jasmine": "^3.3.5",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^10.12.18",
        "codelyzer": "^4.5.0",
        "cross-env": "^5.2.0",
        "gh-pages": "^2.0.1",
        "jasmine-core": "^3.3.0",
        "jasmine-spec-reporter": "^4.2.1",
        "json-schema-faker": "^0.4.0",
        "json-server": "^0.14.2",
        "karma": "^3.1.4",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage-istanbul-reporter": "^2.0.4",
        "karma-jasmine": "^2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "protractor": "^5.4.2",
        "ts-node": "^7.0.1",
        "tslint": "^5.12.0",
        "typescript": "3.1.1"
    }
}
